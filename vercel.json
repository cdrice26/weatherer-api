{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/graphql",
      "dest": "src/main.ts",
      "methods": ["POST", "GET", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}
